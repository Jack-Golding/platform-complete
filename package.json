{
  "name": "@folio/platform-complete",
  "version": "3.3.0",
  "license": "Apache-2.0",
  "scripts": {
    "build": "export NODE_OPTIONS=\"--max-old-space-size=8192 $NODE_OPTIONS\"; stripes build stripes.config.js",
    "stripes": "stripes",
    "start": "stripes serve stripes.config.js",
    "build-module-descriptors": "stripes mod descriptor stripes.config.js --output ./ModuleDescriptors",
    "local": "f=stripes.config.js; test -f $f.local && f=$f.local; echo Using config $f; stripes serve $f",
    "test": "echo 'No unit tests implemented'",
    "test-int": "stripes test nightmare stripes.config.js",
    "test-regression": "stripes test nightmare stripes.config.js --run WD/platform-core/checkout/users/inventory/requests/circulation/tenant-settings",
    "lint": "eslint test/ui-testing"
  },
  "dependencies": {
    "@folio/agreements": ">=6.0.0",
    "@folio/eholdings": "git://github.com/folio-org/ui-eholdings.git#master",
    "@folio/inventory": "git://github.com/folio-org/ui-inventory.git#master",
    "@folio/quick-marc": "git://github.com/folio-org/ui-quick-marc.git#master",
    "@folio/data-import": ">=3.0.0",
    "@folio/myprofile": ">=4.0.0",
    "@folio/notes": ">=5.0.0",
    "@folio/plugin-find-agreement": ">=6.0.0",
    "@folio/plugin-find-package-title": ">=2.0.0",
    "@folio/plugin-find-user": ">=4.0.0",
    "@folio/plugin-find-organization": ">=2.0.0",
    "@folio/stripes": "^6.0.0",
    "@folio/stripes-acq-components": ">=2.0.0",
    "@folio/users": "^6.0.0",
    "moment": "~2.25.3",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "react-intl": "^5.8.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^5.0.0",
    "@folio/stripes-cli": "^2.0.0",
    "eslint": "^6.2.1",
    "lodash": "^4.17.5"
  },
  "resolutions": {
    "rxjs": "^5.4.3",
    "minimist": "^1.2.3",
    "moment": "2.25.3",
    "redux-form": "^8.0.0"
  }
}
